{
    "name": "Scrabble Solver",
    "description": "A Chrome extension that helps you find the best possible words in Scrabble.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "imgs/hh.jpeg"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "extractData.js"
            ],
            "run_at": "document_idle"
        }
    ]
}